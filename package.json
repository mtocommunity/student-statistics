{
  "name": "student-statistics",
  "type": "module",
  "license": "GPL-3.0-only",
  "description": "A web application for managing student statistics.",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev --host",
    "build": "bunx --bun astro check && bunx --bun astro build",
    "preview": "bunx --bun astro preview",
    "astro": "astro",
    "start": "bun --bun dist/server/entry.mjs",
    "format": "prettier --write .",
    "lint": "eslint . --ext .astro,.js,.jsx,.ts,.tsx,.mdx --fix",
    "husky": "husky",
    "db:push": "drizzle-kit generate && bun scripts/migrate.ts"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/node": "9.5.0",
    "@astrojs/react": "4.4.1",
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-toggle": "1.1.10",
    "@svgr/rollup": "8.1.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/vite": "4.1.16",
    "@types/bun": "latest",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vite-pwa/astro": "1.1.1",
    "astro": "5.15.3",
    "astro-compress": "2.3.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-kit": "0.31.6",
    "drizzle-orm": "0.44.7",
    "drizzle-zod": "0.8.3",
    "jose": "6.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "react-icons": "5.5.0",
    "react-number-format": "5.4.4",
    "recharts": "2.15.4",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "3.1.1",
    "tailwindcss": "4.1.16",
    "tw-animate-css": "1.4.0",
    "xlsx": "0.18.5",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.39.0",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.39.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-scope": "8.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-astro-organize-imports": "0.4.11",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.7.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,astro,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
